{
  "$schema": "https://schema.tauri.app/capabilities.json",
  "identifier": "default",
  "description": "Default window capabilities",
  "windows": ["*"],
  "permissions": [
    "core:default",

    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",

    "core:path:default",
    "log:allow-log"
  ]
}
